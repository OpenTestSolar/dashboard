    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
